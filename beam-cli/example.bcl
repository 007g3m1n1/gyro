project: ops-test
serial: 99
environment: production

VpcResource vpc
    resourceName: $(project)-$(serial)-$(environment)
    cidrBlock: "10.0.0.0/16"
end

SubnetResource subnet
    vpc: $(VpcResource vpc)
    availabilityZone: $(VpcResource vpc | region)b
    cidrBlock: "10.0.0.0/24"
    tags:
        belongsTo: $(VpcResource vpc | name)
    end
end

