plugin "git@github.com:perfectsense/beam.git?branch=feature/core-test"

import "vpc.bc" as vpc

aws::subnet public-subnet
    zone: @(aws::vpc vpc.prod-vpc | region)b
    cidr: 10.0.0.0/26
    tags:
        * project: @(aws::vpc vpc.prod-vpc | name) this is a tag
        * serial: $(vpc.SERIAL)
    vpc-id: @(aws::vpc vpc.prod-vpc | vpc-id)
end
