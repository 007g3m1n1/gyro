provider aws
    artifact: "beam:beam-aws-provider:0.5"
    repositories:
        - "http://repo1.maven.org/maven2/"
        - "https://repo.gradle.org/gradle/libs-releases/"
        - "https://artifactory.psdops.com/public"
    end
end

aws::credentials default
    profile-name: psd-sandbox
    region: us-east-1
end

aws::bucket bucket
    name: bucket-example
    enable-object-lock: true
    tags:
        Name: bucket-example-update
        Name2: something
    end
    enable-accelerate-config: true
    enable-version: true
    enable-pay: false
end
