plugin
    artifact: 'beam:beam-rackspace-provider:0.5'
    repositories: [
        'http://repo1.maven.org/maven2/',
        'https://repo.gradle.org/gradle/libs-releases/',
        'https://artifactory.psdops.com/public'
    ]
end

rackspace::credentials default
    profile-name: "psd-beam-sandbox"
    region: "us-east-2"
end

rackspace::aloha aloha-example
    name: "aloha-example"
end